<div class="overflow-auto">
<div class="card" style="width: 25rem;">
  <div class="card-body">
    <% @articles.each do |article| %>
      <h3 class="card-title"><%= article["source"]["name"] %></h3>
      <h4 class="card-subtitle mb-2 text-muted"><%= article["author"]%></h4>
      <p class="card-text"><%= article["description"]%></p>
      <a href= <%= link_to url: article["url"] %> class="card-link" target="_blank">View Article</a>
      <a href="#" class="card-link">Add to Dashboard</a>
    <% end %>
 </div>
  </div>
  </div>